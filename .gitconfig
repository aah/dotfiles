[user]
	name = Shane Watson
	email = shane@watson.io
[alias]
	aa = add --all
	b = branch
	caa = commit --amend -CHEAD
	ci = commit
	co = checkout
	di = diff --cached
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	st = status
[color]
	ui = auto
[color "diff"]
	commit = blue
[core]
	editor = vim
[credential]
	helper = osxkeychain
[merge]
	tool = vimdiff
[pull]
	rebase=true
[push]
	default = simple
